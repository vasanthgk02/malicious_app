<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res/a.SurlyProjectFinal" android:versionCode="5" android:versionName="2.1.2" package="a.SurlyProjectFinal">
    <application android:label="@string/app_name" android:icon="@drawable/intro_centar_image">
        <activity android:label="@string/app_name" android:name="Intro">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".sGalleryActivity"/>
        <activity android:theme="@style/Theme.NoTitleBar.Fullscreen" android:name=".AudioPlayerInSDCardActivity" android:launchMode="singleTop"/>
        <activity android:name="com.inmobi.androidsdk.IMBrowserActivity" android:configChanges="keyboard|keyboardHidden|orientation"/>
        <activity android:name=".Video"/>
        <receiver android:label="MusicPlayeInSDCard" android:name="a.Widget.Widgetmusic">
            <intent-filter>
                <action android:name="action.WIDGET_PREV_TOUCH"/>
                <action android:name="action.WIDGET_PLAY_TOUCH"/>
                <action android:name="action.WIDGET_NEXT_TOUCH"/>
                <action android:name="action.WIDGET_END_APPLICATION"/>
                <action android:name="action.WIDGET_UPDATE_APPLICATION"/>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE"/>
            </intent-filter>
            <meta-data android:name="android.appwidget.provider" android:resource="@xml/widgetmanager"/>
        </receiver>
    </application>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-sdk android:minSdkVersion="8"/>
</manifest>
