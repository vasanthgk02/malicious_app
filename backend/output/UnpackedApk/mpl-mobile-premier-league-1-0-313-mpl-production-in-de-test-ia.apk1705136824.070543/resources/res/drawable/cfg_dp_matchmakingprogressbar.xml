<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:ads="http://schemas.android.com/apk/lib/com.inmobi.ads" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:gravity="left|right|center_vertical|center_horizontal|fill_horizontal|center" android:id="@android:id/background">
        <shape android:tint="?attr/colorControlNormal" android:shape="rectangle">
            <corners android:bottomLeftRadius="8dp" android:bottomRightRadius="8dp"/>
            <size android:height="5dp"/>
            <solid android:color="#ff000000"/>
        </shape>
    </item>
    <item android:gravity="left|right|center_vertical|center_horizontal|fill_horizontal|center" android:id="@android:id/progress">
        <scale android:scaleWidth="100%">
            <shape android:tint="?attr/colorControlActivated" android:shape="rectangle">
                <corners android:bottomLeftRadius="8dp" android:bottomRightRadius="8dp"/>
                <size android:height="5dp"/>
                <solid android:color="#ffffffff"/>
            </shape>
        </scale>
    </item>
</layer-list>
