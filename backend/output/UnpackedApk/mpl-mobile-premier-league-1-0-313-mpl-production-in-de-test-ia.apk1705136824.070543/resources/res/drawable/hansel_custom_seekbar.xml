<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:ads="http://schemas.android.com/apk/lib/com.inmobi.ads" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:gravity="left|right|center_vertical|center_horizontal|fill_horizontal|center" android:id="@+id/item_bg">
        <shape android:shape="rectangle">
            <size android:height="1dp"/>
            <solid android:color="@color/hansel_grey_line"/>
        </shape>
    </item>
    <item android:gravity="left|right|center_vertical|center_horizontal|fill_horizontal|center" android:id="@+id/item_prog">
        <scale android:scaleWidth="100%">
            <shape android:shape="rectangle">
                <size android:height="1dp"/>
                <solid android:color="@color/hansel_sky_blue"/>
            </shape>
        </scale>
    </item>
</layer-list>
