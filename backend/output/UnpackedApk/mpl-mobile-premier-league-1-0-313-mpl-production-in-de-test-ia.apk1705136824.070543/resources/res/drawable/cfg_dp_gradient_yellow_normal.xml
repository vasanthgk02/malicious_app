<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:ads="http://schemas.android.com/apk/lib/com.inmobi.ads" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#22000000"/>
            <corners android:radius="32dp"/>
            <stroke android:width="2dp" android:color="#22000000"/>
        </shape>
    </item>
    <item android:right="1.5dp" android:bottom="2dp">
        <shape android:shape="rectangle">
            <corners android:radius="32dp"/>
            <gradient android:startColor="#f2ffc107" android:endColor="#ffffeb3b" android:useLevel="true" android:angle="90" android:type="linear"/>
        </shape>
    </item>
</layer-list>
